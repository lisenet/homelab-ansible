  append initrd=/networkboot/{{ networkboot_directory_ubuntu1804server }}/initrd.gz ks=nfs:{{ ansible_default_ipv4.address }}:/var/nfs/{{ networkboot_directory_ubuntu1804server }}/preseed/{{ kickstart_file_ubuntu1804server }} ---
label Install Ubuntu 18.04 LTS Desktop
