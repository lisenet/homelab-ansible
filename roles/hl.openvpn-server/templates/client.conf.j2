{{ ansible_managed | comment }}
# Generated by Ansible role {{ ansible_role_name }}

client
remote PLACEHOLDER
iproto {{ openvpn_protocol }}
port {{ openvpn_port }}
dev tun
nobind
reneg-sec 0
remote-cert-tls server
key-direction 1
auth SHA512
cipher {{ openvpn_cipher }}
verb 3

# Redirect changes client routing table so that all traffic is directed via server
redirect-gateway def1

